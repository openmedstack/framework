<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFrameworks>net5.0</TargetFrameworks>
        <RootNamespace>OpenMedStack.Autofac.MassTransit.ActiveMq</RootNamespace>
        <PackageId>OpenMedStack.Autofac.MassTransit.ActiveMq</PackageId>
        <PackageTags>OpenMedStack Architecture Messaging MassTransit ActiveMQ</PackageTags>
        <PackageReleaseNotes>Packaged with nubian for Debian compatibility.</PackageReleaseNotes>
        <Description>Autofac bindings for OpenMedStack based applications.</Description>
        <Nullable>enable</Nullable>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>
    <Import Project="..\..\shared.proj" />

    <ItemGroup>
        <PackageReference Include="MassTransit.ActiveMQ" Version="7.2.0" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\openmedstack.autofac.masstransit\openmedstack.autofac.masstransit.csproj" />
    </ItemGroup>

</Project>
